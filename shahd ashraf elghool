def selection_sort(arr):
    n = len(arr)
    for i in range(n - 1):
        min_index = i
        for j in range(i + 1, n):
            if arr[j] < arr[min_index]:
                min_index = j
        arr[i], arr[min_index] = arr[min_index], arr[i]

def main():
    arr = [10, 12, 8, 15, 6]
    selection_sort(arr)
    print(arr, "list after order")

if __name__ == "__main__":
    main()
